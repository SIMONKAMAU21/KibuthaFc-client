{
  "expo": {
    "name": "kibuthafc",
    "slug": "kibuthafc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/kibuthaicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/kibuthaicon.png",
        "backgroundColor": "#0000"
      },
      "package": "com.jahnnet.frontend"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/kibuthafc.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0000"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d580cb87-c604-46dd-850f-d69d6d1bbc91"
      }
    },
    "owner": "jahnnet",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d580cb87-c604-46dd-850f-d69d6d1bbc91"
    }
  }
}
